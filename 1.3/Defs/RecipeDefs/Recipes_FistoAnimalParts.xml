  <RecipeDef ParentName="BionicAnimalCraftBase">
    <defName>MakeEnhancedProtectronDrill</defName>
    <label>make enhanced Protectron drill</label>
    <description>Make a enhanced Protectron drill for Fisto.</description>
    <jobString>Making enhanced protectron drill.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>ComponentSpacer</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <EnhancedProtectronDrill>1</EnhancedProtectronDrill>
    </products>
    <skillRequirements>
      <Crafting>9</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
